{
    "version": 2,
    "name": "Axar",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "functions": {
        "index.js": {
            "maxDuration": 30
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}     
